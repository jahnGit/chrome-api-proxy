{
  "name": "Chrome API Proxy - Agent",
  "version": "0.0.4",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": [
      "contentScript.js"
    ],
    "run_at": "document_start"
  }],
  "icons": {
    "128": "assets/icon_128.png"
  }
}
